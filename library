ll gcd(ll a, ll b, ll & x, ll & y) {
    if (a == 0) {
        x = 0;
        y = 1;
        return b;
    }
    ll x1, y1;
    ll d = gcd(b % a, a, x1, y1);
    x = y1 - (b / a) * x1;
    y = x1;
    return d;
}


ll sieve(ll n)
{
  ll primes[n+1];
  fill(primes+primes+n+1,0);
  for(ll i=2;i*i<=n;i++)
  {
  	if(primes[i]==0)
  	{for(ll j=i;j<=n;j++)
  		primes[i*j]=1;}
   }

}

