ll Power(ll base , ll e)
{
    if(e == 0) return 1;
    ll ans = base; e--;
    while(e)
	{
        if(e & 1) ans = (ans * base) %mod;
        base = (base * base) %mod;
        e >>= 1;
	}
	return ans % mod;
}
