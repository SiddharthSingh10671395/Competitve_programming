// pointer node can directly to reference without using & with new node

struct trie
{ 
    trie* children[26];
    bool child[26];
  
    // isEndOfWord is true if the node represents 
    // end of a word 
    bool is;
}; 
  
// Returns new trie node (initialized to NULLs) 
trie* getNode(void) 
{ 
    trie *pNode =  new trie; 
  
    
  
    for (int i = 0; i < 26; i++) {
        pNode->child[i] = false;
        pNode->children[i]=NULL; 
    }
    pNode->is=false;
  
    return pNode; 
} 
