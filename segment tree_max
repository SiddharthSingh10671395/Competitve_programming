std::vector<int> seg(N*4);
 
void sege(int x,int y,int pos)
{
if(x==y){
    
    seg[pos]=maax[x]; 
    return;
        
}
int mid=(x+y)/2;
sege(x,mid,2*pos +1);
sege(mid+1,y,2*pos+2);
seg[pos]=max(seg[2*pos+1],seg[2*pos+2]);

}
 
int qr(int x,int y,int lo,int hi,int pos)
{
 
    if(lo>=x&&hi<=y) return seg[pos];
 
    if(hi<x||lo>y) return 0;
 
    int mid=(lo+hi)/2;
 
    return max(qr(x,y,lo,mid,2*pos+1),qr(x,y,mid+1,hi,2*pos+2));
 
}
